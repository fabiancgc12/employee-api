{
  "name": "employeeapi",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "npx rimraf ./dist/",
    "prebuild": "npm run clean",
    "start": "ts-node --esm bin/www.ts",
    "start:prod": "node dist/bin/www",
    "postbuild": "yarn run copyPublic",
    "copyPublic": "copyfiles --error --up 1 public/**/* dist/public",
    "build": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": ">2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-sass-middleware": "1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
